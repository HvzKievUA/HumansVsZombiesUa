style.
	img.qr {position: absolute; top: 100px; left: 17px;}
	span.number {position: absolute; top: 106px; left: 174px; }
	span.hash {position: absolute; top: 142px; left: 142px;}
	@media print {table {page-break-after: always;}}
	span {font-weight: bold;}
- for (var page = 0; page < codes.length; page++)
	p Сторінка #{page + 1} з #{codes.length}
	table
		tr
			-var code = codes[page]
			td(width="347px", style="position: relative;")
				if code
					img(src="/img/card.png", width="347px")
					span.hash=code.hash
					span.number=code.number
					img.qr(src="https://api.qrserver.com/v1/create-qr-code/?size=80x80&data=http://www.hvz.kiev.ua/eat/#{code.hash}")
